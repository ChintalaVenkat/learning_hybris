<?xml version="1.0" encoding="ISO-8859-1"?>
<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="items.xsd">

	<itemtypes>
		<itemtype code="AbstractOrderEntry" autocreate="false"
			generate="false">
			<attributes>
				<attribute qualifier="externalConfiguration" type="java.lang.String">
					<description>XML Representation of a Product Configuration</description>
					<modifiers read="true" write="true" search="false"
						optional="true" />
					<persistence type="property">
						<columntype database="oracle">
							<value>CLOB</value>
						</columntype>
						<columntype database="sap">
							<value>NCLOB</value>
						</columntype>
						<columntype database="mysql">
							<value>TEXT</value>
						</columntype>
						<columntype>
							<value>HYBRIS.LONG_STRING</value>
						</columntype>
					</persistence>
				</attribute>
			</attributes>
		</itemtype>
		
	</itemtypes>
</items>
